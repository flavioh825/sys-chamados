<form class="form-signin" method="POST" action="/rescue">
  <h2 class="form-signin-heading">Esqueci a senha</h2>
  
  	<%- req.flash('erro') %>
  	<%- req.flash('info') %>
  
  <input type="email" name="email" class="form-control" placeholder="Digite seu e-mail" required autofocus>
  <input type="submit" class="btn btn-lg btn-primary btn-block" value="Enviar">
</form>