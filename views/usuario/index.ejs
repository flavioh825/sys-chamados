<h3>Usuarios</h3>
<table border="1">
    <tr>
        <th>#</th>
        <th>Nome</th>
        <th>E-mail</th>
        <th>ramal</th>
        <th>Departamento</th>
        <th>Unidade</th>
        <th>Atende</th>
        <th></th>
    </tr>
    <% _.each(usuario, function(user){ %>
    <tr data-id="<%= user.id %>" data-model="usuario">
        <td><%= user.id %></td>
        <td><%= user.nome %> <%= user.sobrenome %></td>
        <td><%= user.email %></td>
        <td><%= user.ramal %></td>
        <td><%= user.iddepartamento.nome %></td>
        <td><%= user.iddepartamento.unidade %></td>
        <td><%= (user.responde_chamado != 1)? 'NÃ£o': 'Sim' %></td>
        <td>
            <a href="/usuario/show/<%= user.id %>" >Show</a> |
            <a href="/usuario/edit/<%= user.id %>" >Edit</a> |
            <a href="/usuario/destroy/<%= user.id %>" >Delete</a>
        </td>
    </tr>
    <% }) %>
</table>